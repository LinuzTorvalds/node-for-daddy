// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  code     String   @id
  name     String
  email    String   @unique
  password String
  token    String?
}

model material {
  lote     String @id
  description String
  amount   Int 
  shelf_life DateTime @db.Date
}

model reports {
  code     String   @id
  lote     String
  description     String
  amount   Int  
  day DateTime @db.Date
}
